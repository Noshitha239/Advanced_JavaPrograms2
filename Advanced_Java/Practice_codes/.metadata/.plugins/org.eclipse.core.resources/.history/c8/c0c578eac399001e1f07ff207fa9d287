
public class removeDuplicateNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int r=0;
		int ar[]= {10,30,10,20,30,50};
		for(int i=1;i<ar.length;i++) {
			if(ar[r]!=ar[i]) {
				r++;
				ar[r]=ar[i];
			}
		}
		 for(int i=1;i<ar.length;i++) {
			 System.out.println(ar[i]);
		 }
		
//	        for(int i=0;i<=r;i++) {
//	        	System.out.println(ar[i]);
//	        }
	}

}
